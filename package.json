{
  "name": "@dandre3000/testing",
  "version": "1.0.4",
  "license": "MIT",
  "author": {
    "name": "DeAundre Payne",
    "email": "apps.fondling796@passinbox.com",
    "url": "https://github.com/dandre3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandre3000/testing.git"
  },
  "description": "A collection of runtime agnostic testing utilities with a jest-like api. Exports describe, it, expect, an assertion library and an alias for the test runner of the current runtime.",
  "keywords": [
    "test",
    "jest",
    "describe",
    "it",
    "expect",
    "assert",
    "node",
    "bun",
    "deno"
  ],
  "files": [
    ".npmrc",
    "bunfig.toml",
    "./node.js",
    "./deno.js",
    "./bun.js",
    "./testing.d.ts",
    "./bin"
  ],
  "exports": {
    ".": {
      "bun": {
        "types": "./testing.d.ts",
        "import": "./bun.js"
      },
      "deno": {
        "types": "./testing.d.ts",
        "import": "./deno.js"
      },
      "default": {
        "types": "./testing.d.ts",
        "import": "./node.js"
      }
    },
    "./types": "./testing.d.ts",
    "./package.json": "./package.json"
  },
  "bin": {
    "tester": "bin/tester.js"
  },
  "dependencies": {
    "@std/assert": "npm:@jsr/std__assert@^1.0.15",
    "@std/expect": "npm:@jsr/std__expect@^1.0.17",
    "cross-bdd": "^0.2.0"
  },
  "devDependencies": {
    "@types/deno": "^2.5.0",
    "@types/bun": "^1.3.1"
  },
  "type": "module"
}
